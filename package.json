{
  "name": "ollama-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "bun run jest",
    "test:watch": "bun run jest --watch",
    "test:coverage": "bun run jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "bun run test && bun run test:e2e",
    "test:local": "bun scripts/test-local.js",
    "setup-dev-env": "bun scripts/setup-dev-env.js",
    "validate-env": "bun scripts/validate-dev-env.js",
    "prepare": "husky || true",
    "ci:install": "bun install --frozen-lockfile",
    "ci:test": "bun run lint && bun run test:coverage && bun run build",
    "ci:e2e": "bun run test:e2e",
    "security:audit": "bun audit --audit-level=moderate",
    "deps:check": "bun npm-check-updates",
    "deps:update": "bun npm-check-updates -u && bun install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "next": "15.1.8",
    "next-auth": "^5.0.0-beta.28",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "@eslint/eslintrc": "^3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.49.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.14",
    "msw": "^2.6.8",
    "whatwg-fetch": "^3.6.20",
    "ts-jest": "^29.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "prettier": "^3.2.5",
    "axe-playwright": "^2.0.1",
    "lighthouse": "^11.7.0",
    "audit-ci": "^7.0.1",
    "npm-check-updates": "^16.14.15"
  }
}
